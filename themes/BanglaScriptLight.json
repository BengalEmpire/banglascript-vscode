{
  "$schema": "vscode://schemas/color-theme",
  "name": "BanglaScript Light Pro",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#24292F",
    "editor.lineHighlightBackground": "#F6F8FA",
    "editor.selectionBackground": "#0969DA33",
    "editor.selectionHighlightBackground": "#1A7F3722",
    "editorCursor.foreground": "#0969DA",
    "editorLineNumber.foreground": "#8C959F",
    "editorLineNumber.activeForeground": "#24292F",
    "editorIndentGuide.background1": "#D8DEE4",
    "editorIndentGuide.activeBackground1": "#1A7F37",
    "editorBracketMatch.background": "#1A7F3733",
    "editorBracketMatch.border": "#1A7F37",
    "editorWhitespace.foreground": "#D8DEE455",
    "activityBar.background": "#FFFFFF",
    "activityBar.foreground": "#24292F",
    "activityBar.border": "#D8DEE4",
    "activityBarBadge.background": "#1A7F37",
    "activityBarBadge.foreground": "#FFFFFF",
    "sideBar.background": "#F6F8FA",
    "sideBar.foreground": "#24292F",
    "sideBar.border": "#D8DEE4",
    "sideBarTitle.foreground": "#24292F",
    "statusBar.background": "#FFFFFF",
    "statusBar.foreground": "#57606A",
    "statusBar.border": "#D8DEE4",
    "titleBar.activeBackground": "#F6F8FA",
    "titleBar.activeForeground": "#24292F",
    "titleBar.border": "#D8DEE4",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeForeground": "#24292F",
    "tab.inactiveBackground": "#F6F8FA",
    "tab.inactiveForeground": "#57606A",
    "tab.activeBorderTop": "#FD8C73",
    "tab.border": "#D8DEE4",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#24292F",
    "panel.background": "#FFFFFF",
    "panel.border": "#D8DEE4",
    "badge.background": "#1A7F37",
    "badge.foreground": "#FFFFFF",
    "button.background": "#1A7F37",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#2DA44E",
    "dropdown.background": "#FFFFFF",
    "dropdown.border": "#D0D7DE",
    "input.background": "#FFFFFF",
    "input.border": "#D0D7DE",
    "input.foreground": "#24292F",
    "input.placeholderForeground": "#6E7781",
    "editorWidget.background": "#FFFFFF",
    "editorWidget.border": "#D0D7DE",
    "editorSuggestWidget.background": "#FFFFFF",
    "editorSuggestWidget.border": "#D0D7DE",
    "editorSuggestWidget.highlightForeground": "#0969DA",
    "editorSuggestWidget.selectedBackground": "#DDF4FF",
    "list.activeSelectionBackground": "#DDF4FF",
    "list.activeSelectionForeground": "#24292F",
    "list.hoverBackground": "#F6F8FA",
    "list.focusBackground": "#DDF4FF",
    "focusBorder": "#0969DA",
    "gitDecoration.addedResourceForeground": "#1A7F37",
    "gitDecoration.modifiedResourceForeground": "#9A6700",
    "gitDecoration.deletedResourceForeground": "#CF222E",
    "gitDecoration.untrackedResourceForeground": "#1A7F37",
    "minimap.background": "#FFFFFF",
    "scrollbarSlider.background": "#8C959F33",
    "scrollbarSlider.hoverBackground": "#8C959F55",
    "scrollbarSlider.activeBackground": "#8C959F77"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.line",
        "comment.block"
      ],
      "settings": {
        "foreground": "#6E7781",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#0A3069"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#116329"
      }
    },
    {
      "name": "Numbers - All",
      "scope": [
        "constant.numeric",
        "constant.numeric.bengali",
        "constant.numeric.decimal",
        "constant.numeric.hex",
        "constant.numeric.binary",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "name": "Boolean True",
      "scope": "constant.language.boolean.true",
      "settings": {
        "foreground": "#1A7F37",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Boolean False",
      "scope": "constant.language.boolean.false",
      "settings": {
        "foreground": "#CF222E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Null & Undefined",
      "scope": [
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#CF222E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Control Keywords - Conditional",
      "scope": "keyword.control.conditional",
      "settings": {
        "foreground": "#CF222E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords - Loop",
      "scope": "keyword.control.loop",
      "settings": {
        "foreground": "#CF222E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords - Flow",
      "scope": "keyword.control.flow",
      "settings": {
        "foreground": "#CF222E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords - Try/Catch",
      "scope": "keyword.control.trycatch",
      "settings": {
        "foreground": "#CF222E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords - Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#CF222E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Types - Variables",
      "scope": "storage.type",
      "settings": {
        "foreground": "#8250DF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Types - Function",
      "scope": "storage.type.function",
      "settings": {
        "foreground": "#8250DF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Types - Class",
      "scope": "storage.type.class",
      "settings": {
        "foreground": "#8250DF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Types - Async",
      "scope": "storage.type.async",
      "settings": {
        "foreground": "#8250DF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Modifiers",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#CF222E"
      }
    },
    {
      "name": "Keyword - New",
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#CF222E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword - Await",
      "scope": "keyword.control.await",
      "settings": {
        "foreground": "#8250DF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword - Throw",
      "scope": "keyword.control.throw",
      "settings": {
        "foreground": "#CF222E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable - This",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#953800",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable - Super",
      "scope": "variable.language.super",
      "settings": {
        "foreground": "#953800",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable - Other",
      "scope": "variable.other",
      "settings": {
        "foreground": "#24292F"
      }
    },
    {
      "name": "Function Names",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "Support - Console Functions",
      "scope": "support.function.console",
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "name": "Support - DOM Variables",
      "scope": "support.variable.dom",
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "name": "Support - DOM Functions",
      "scope": "support.function.dom",
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "Support - Browser Functions",
      "scope": "support.function.browser",
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "name": "Support - Array Methods",
      "scope": "support.function.array",
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "Support - String Methods",
      "scope": "support.function.string",
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "Support - Math Class",
      "scope": "support.class.math",
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "name": "Support - Math Functions",
      "scope": "support.function.math",
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "name": "Support - Promise",
      "scope": [
        "support.class.promise",
        "support.function.promise"
      ],
      "settings": {
        "foreground": "#0550AE"
      }
    },
    {
      "name": "Operators - Word",
      "scope": [
        "keyword.operator.typeof",
        "keyword.operator.instanceof",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.delete",
        "keyword.operator.logical",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#CF222E"
      }
    },
    {
      "name": "Operators - Symbols",
      "scope": [
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.ternary",
        "keyword.operator.spread"
      ],
      "settings": {
        "foreground": "#CF222E"
      }
    },
    {
      "name": "Punctuation - Separators",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#57606A"
      }
    },
    {
      "name": "Punctuation - Braces",
      "scope": [
        "meta.brace.curly",
        "meta.brace.square",
        "meta.brace.round"
      ],
      "settings": {
        "foreground": "#24292F"
      }
    },
    {
      "name": "Template Literal Expression",
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#24292F"
      }
    }
  ],
  "semanticTokenColors": {
    "function": "#8250DF",
    "function.declaration": "#8250DF",
    "variable": "#24292F",
    "variable.declaration": "#24292F",
    "parameter": "#953800",
    "property": "#0550AE",
    "class": "#953800",
    "interface": "#953800",
    "type": "#953800",
    "enum": "#953800",
    "enumMember": "#0550AE"
  }
}