{
  "$schema": "vscode://schemas/color-theme",
  "name": "BanglaScript Dark Pro",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#0D1117",
    "editor.foreground": "#E6EDF3",
    "editor.lineHighlightBackground": "#161B22",
    "editor.selectionBackground": "#264F78",
    "editor.selectionHighlightBackground": "#3FB95044",
    "editorCursor.foreground": "#58A6FF",
    "editorLineNumber.foreground": "#484F58",
    "editorLineNumber.activeForeground": "#E6EDF3",
    "editorIndentGuide.background1": "#21262D",
    "editorIndentGuide.activeBackground1": "#3FB950",
    "editorBracketMatch.background": "#3FB95033",
    "editorBracketMatch.border": "#3FB950",
    "editorWhitespace.foreground": "#484F5855",
    "activityBar.background": "#0D1117",
    "activityBar.foreground": "#E6EDF3",
    "activityBarBadge.background": "#3FB950",
    "activityBarBadge.foreground": "#0D1117",
    "sideBar.background": "#010409",
    "sideBar.foreground": "#C9D1D9",
    "sideBarTitle.foreground": "#E6EDF3",
    "statusBar.background": "#0D1117",
    "statusBar.foreground": "#8B949E",
    "statusBar.border": "#21262D",
    "titleBar.activeBackground": "#0D1117",
    "titleBar.activeForeground": "#E6EDF3",
    "tab.activeBackground": "#0D1117",
    "tab.activeForeground": "#E6EDF3",
    "tab.inactiveBackground": "#010409",
    "tab.inactiveForeground": "#8B949E",
    "tab.activeBorderTop": "#F78166",
    "terminal.background": "#0D1117",
    "terminal.foreground": "#E6EDF3",
    "panel.background": "#0D1117",
    "panel.border": "#21262D",
    "badge.background": "#3FB950",
    "badge.foreground": "#0D1117",
    "button.background": "#238636",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#2EA043",
    "dropdown.background": "#161B22",
    "dropdown.border": "#30363D",
    "input.background": "#0D1117",
    "input.border": "#30363D",
    "input.foreground": "#E6EDF3",
    "input.placeholderForeground": "#484F58",
    "editorWidget.background": "#161B22",
    "editorWidget.border": "#30363D",
    "editorSuggestWidget.background": "#161B22",
    "editorSuggestWidget.border": "#30363D",
    "editorSuggestWidget.highlightForeground": "#58A6FF",
    "editorSuggestWidget.selectedBackground": "#21262D",
    "list.activeSelectionBackground": "#21262D",
    "list.activeSelectionForeground": "#E6EDF3",
    "list.hoverBackground": "#161B22",
    "list.focusBackground": "#21262D",
    "focusBorder": "#1F6FEB",
    "gitDecoration.addedResourceForeground": "#3FB950",
    "gitDecoration.modifiedResourceForeground": "#D29922",
    "gitDecoration.deletedResourceForeground": "#F85149",
    "gitDecoration.untrackedResourceForeground": "#3FB950",
    "minimap.background": "#0D1117",
    "scrollbarSlider.background": "#484F5855",
    "scrollbarSlider.hoverBackground": "#484F5888",
    "scrollbarSlider.activeBackground": "#484F58AA"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.line",
        "comment.block"
      ],
      "settings": {
        "foreground": "#8B949E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#A5D6FF"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#7EE787"
      }
    },
    {
      "name": "Numbers - All",
      "scope": [
        "constant.numeric",
        "constant.numeric.bengali",
        "constant.numeric.decimal",
        "constant.numeric.hex",
        "constant.numeric.binary",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "Boolean True",
      "scope": "constant.language.boolean.true",
      "settings": {
        "foreground": "#3FB950",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Boolean False",
      "scope": "constant.language.boolean.false",
      "settings": {
        "foreground": "#F85149",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Null & Undefined",
      "scope": [
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#FF7B72",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Control Keywords - Conditional",
      "scope": "keyword.control.conditional",
      "settings": {
        "foreground": "#FF7B72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords - Loop",
      "scope": "keyword.control.loop",
      "settings": {
        "foreground": "#FF7B72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords - Flow",
      "scope": "keyword.control.flow",
      "settings": {
        "foreground": "#FF7B72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords - Try/Catch",
      "scope": "keyword.control.trycatch",
      "settings": {
        "foreground": "#FF7B72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords - Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#FF7B72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Types - Variables",
      "scope": "storage.type",
      "settings": {
        "foreground": "#D2A8FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Types - Function",
      "scope": "storage.type.function",
      "settings": {
        "foreground": "#D2A8FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Types - Class",
      "scope": "storage.type.class",
      "settings": {
        "foreground": "#D2A8FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Types - Async",
      "scope": "storage.type.async",
      "settings": {
        "foreground": "#D2A8FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Modifiers",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Keyword - New",
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#FF7B72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword - Await",
      "scope": "keyword.control.await",
      "settings": {
        "foreground": "#D2A8FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword - Throw",
      "scope": "keyword.control.throw",
      "settings": {
        "foreground": "#F85149",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable - This",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#FFA657",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable - Super",
      "scope": "variable.language.super",
      "settings": {
        "foreground": "#FFA657",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable - Other",
      "scope": "variable.other",
      "settings": {
        "foreground": "#E6EDF3"
      }
    },
    {
      "name": "Function Names",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Support - Console Functions",
      "scope": "support.function.console",
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "Support - DOM Variables",
      "scope": "support.variable.dom",
      "settings": {
        "foreground": "#FFA657"
      }
    },
    {
      "name": "Support - DOM Functions",
      "scope": "support.function.dom",
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Support - Browser Functions",
      "scope": "support.function.browser",
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "Support - Array Methods",
      "scope": "support.function.array",
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Support - String Methods",
      "scope": "support.function.string",
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Support - Math Class",
      "scope": "support.class.math",
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "Support - Math Functions",
      "scope": "support.function.math",
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Support - Promise",
      "scope": [
        "support.class.promise",
        "support.function.promise"
      ],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "Operators - Word",
      "scope": [
        "keyword.operator.typeof",
        "keyword.operator.instanceof",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.delete",
        "keyword.operator.logical",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Operators - Symbols",
      "scope": [
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.ternary",
        "keyword.operator.spread"
      ],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Punctuation - Separators",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#8B949E"
      }
    },
    {
      "name": "Punctuation - Braces",
      "scope": [
        "meta.brace.curly",
        "meta.brace.square",
        "meta.brace.round"
      ],
      "settings": {
        "foreground": "#E6EDF3"
      }
    },
    {
      "name": "Template Literal Expression",
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#E6EDF3"
      }
    }
  ],
  "semanticTokenColors": {
    "function": "#D2A8FF",
    "function.declaration": "#D2A8FF",
    "variable": "#E6EDF3",
    "variable.declaration": "#E6EDF3",
    "parameter": "#FFA657",
    "property": "#79C0FF",
    "class": "#F0883E",
    "interface": "#F0883E",
    "type": "#F0883E",
    "enum": "#F0883E",
    "enumMember": "#79C0FF"
  }
}